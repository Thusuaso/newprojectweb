{"ast":null,"code":"import service from \"@/service/RaporService\";\nimport { mapGetters } from \"vuex\";\nimport { FilterMatchMode } from \"primevue/api\";\nexport default {\n  data() {\n    return {\n      ureticiDagilimi: null,\n      selectedYear: 2022,\n      years: [{\n        year: 2022\n      }, {\n        year: 2021\n      }, {\n        year: 2020\n      }],\n      topTedarikciMiktar: 0,\n      filters: {\n        tedarikci: {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        }\n      }\n    };\n  },\n  computed: {\n    ...mapGetters([\"servis_adres\"])\n  },\n  methods: {\n    isUreticiDagilimToplami(event) {\n      this.topTedarikciMiktar = 0;\n      console.log(event.filteredValue);\n      for (let i in event.filteredValue) {\n        this.topTedarikciMiktar += event.filteredValue[i].topUretimBedel;\n      }\n    },\n    ureticiDagilimiService(year) {\n      service.getUreticiDagilimi(year).then(data => {\n        this.ureticiDagilimi = data;\n        this.miktarTopla(data);\n      });\n    },\n    isSelectedYear(event) {\n      this.ureticiDagilimiService(event.value.year);\n    },\n    formatDecimal(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    formatPrice(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return \"$\" + val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    miktarTopla(data) {\n      this.topTedarikciMiktar = 0;\n      for (let item of data) {\n        this.topTedarikciMiktar += item.topUretimBedel;\n      }\n    },\n    get_tedarikci_excell_cikti_btn() {\n      service.get_tedarikci_list_excell_cikti(this.ureticiDagilimi).then(response => {\n        if (response.status) {\n          const link = document.createElement(\"a\");\n          link.href = this.servis_adres + \"islemler/listeler/ureticiDagilimiExcelList\";\n          link.setAttribute(\"download\", \"uretimTedarikciRapor.xlsx\");\n          document.body.appendChild(link);\n          link.click();\n          this.is_excel = false;\n        }\n      });\n    }\n  },\n  created() {\n    this.ureticiDagilimiService(this.selectedYear);\n    this.selectedYear = {\n      year: 2022\n    };\n  }\n};","map":{"version":3,"mappings":"AA8DA,OAAOA,OAAM,MAAO,wBAAwB;AAC5C,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,eAAc,QAAS,cAAc;AAC9C,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,eAAe,EAAE,IAAI;MACrBC,YAAY,EAAE,IAAI;MAClBC,KAAK,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAK,CAAC,EAAE;QAAEA,IAAI,EAAE;MAAK,CAAC,EAAE;QAAEA,IAAI,EAAE;MAAK,CAAC,CAAC;MACvDC,kBAAkB,EAAE,CAAC;MACrBC,OAAO,EAAE;QACPC,SAAS,EAAE;UAAEC,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAEV,eAAe,CAACW;QAAY;MACnE;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGb,UAAU,CAAC,CAAC,cAAc,CAAC;EAChC,CAAC;EACDc,OAAO,EAAE;IACPC,uBAAuB,CAACC,KAAK,EAAE;MAC7B,IAAI,CAACT,kBAAiB,GAAI;MAC1BU,OAAO,CAACC,GAAG,CAACF,KAAK,CAACG,aAAa;MAC/B,KAAK,IAAIC,KAAKJ,KAAK,CAACG,aAAa,EAAE;QACjC,IAAI,CAACZ,kBAAiB,IAAKS,KAAK,CAACG,aAAa,CAACC,CAAC,CAAC,CAACC,cAAa;MACjE;IACF,CAAC;IACDC,sBAAsB,CAAChB,IAAI,EAAE;MAC3BP,OAAO,CAACwB,kBAAkB,CAACjB,IAAI,CAAC,CAACkB,IAAI,CAAEtB,IAAI,IAAK;QAC9C,IAAI,CAACC,eAAc,GAAID,IAAI;QAC3B,IAAI,CAACuB,WAAW,CAACvB,IAAI,CAAC;MACxB,CAAC,CAAC;IACJ,CAAC;IACDwB,cAAc,CAACV,KAAK,EAAE;MACpB,IAAI,CAACM,sBAAsB,CAACN,KAAK,CAACN,KAAK,CAACJ,IAAI,CAAC;IAC/C,CAAC;IACDqB,aAAa,CAACjB,KAAK,EAAE;MACnB,IAAIkB,GAAE,GAAI,CAAClB,KAAI,GAAI,CAAC,EAAEmB,OAAO,CAAC,CAAC,CAAC,CAACC,OAAO,CAAC,GAAG,EAAE,GAAG,CAAC;MAClD,OAAOF,GAAG,CAACG,QAAQ,EAAE,CAACD,OAAO,CAAC,uBAAuB,EAAE,GAAG,CAAC;IAC7D,CAAC;IAEDE,WAAW,CAACtB,KAAK,EAAE;MACjB,IAAIkB,GAAE,GAAI,CAAClB,KAAI,GAAI,CAAC,EAAEmB,OAAO,CAAC,CAAC,CAAC,CAACC,OAAO,CAAC,GAAG,EAAE,GAAG,CAAC;MAClD,OAAO,GAAE,GAAIF,GAAG,CAACG,QAAQ,EAAE,CAACD,OAAO,CAAC,uBAAuB,EAAE,GAAG,CAAC;IACnE,CAAC;IACDL,WAAW,CAACvB,IAAI,EAAE;MAChB,IAAI,CAACK,kBAAiB,GAAI,CAAC;MAC3B,KAAK,IAAI0B,IAAG,IAAK/B,IAAI,EAAE;QACrB,IAAI,CAACK,kBAAiB,IAAK0B,IAAI,CAACZ,cAAc;MAChD;IACF,CAAC;IACDa,8BAA8B,