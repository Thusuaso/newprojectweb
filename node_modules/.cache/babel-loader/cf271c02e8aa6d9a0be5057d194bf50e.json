n\r\n            field=\"boyut\"\r\n            header=\"EnxBoyxKenar\"\r\n            headerStyle=\"width:12%;\"\r\n            bodyStyle=\"text-align:left\"\r\n          >\r\n          </Column>\r\n          <Column\r\n            field=\"miktar\"\r\n            header=\"Miktar\"\r\n            headerStyle=\"width:9%;\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ formatDecimal(slotProps.data.miktar) }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"birim\"\r\n            header=\"Birim\"\r\n            headerStyle=\"width:5%;\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <div :style=\"{ backgroundColor: slotProps.data.birimBackground }\">\r\n                {{ slotProps.data.birim }}\r\n              </div>\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"uretimMiktari\"\r\n            header=\"Üretim Miktarı\"\r\n            headerStyle=\"width:8%;\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ formatDecimal(slotProps.data.uretimMiktari) }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"uretimAdet\"\r\n            header=\"Üretim Adet\"\r\n            headerStyle=\"width:8%;\"\r\n            bodyStyle=\"text-align:center\"\r\n          ></Column>\r\n          <Column\r\n            field=\"kalanBilgisi\"\r\n            header=\"Kalan Bilgisi\"\r\n            headerStyle=\"width:12%;\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <div\r\n                :style=\"{\r\n                  backgroundColor: slotProps.data.kalanRenk,\r\n                  color: slotProps.data.fontColor,\r\n                }\"\r\n              >\r\n                {{ slotProps.data.kalanBilgisi }}\r\n              </div>\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"kalanMiktar\"\r\n            header=\"Kalan Miktar\"\r\n            headerStyle=\"width:8%;\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ formatDecimal(slotProps.data.kalanMiktar) }}\r\n            </template>\r\n          </Column>\r\n\r\n          <Column\r\n            field=\"kalanAdet\"\r\n            header=\"Kalan Adet\"\r\n            headerStyle=\"width:8%;\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.kalanAdet }}\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport service from \"@/service/RaporService\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      is_select_year: false,\r\n      selectedPo: null,\r\n      orders: [],\r\n      products: null,\r\n      filteredOrders: [],\r\n    };\r\n  },\r\n  created() {\r\n    service.getAllOrders().then((data) => {\r\n      this.orders = data;\r\n    });\r\n  },\r\n  methods: {\r\n    isSelectedPo(event) {\r\n      service.getOrder(event.value.siparisNo).then((data) => {\r\n        this.products = data;\r\n        this.is_select_year = true;\r\n      });\r\n    },\r\n    formatDecimal(value) {\r\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\r\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\r\n    },\r\n    poChangeEvent(event) {\r\n      setTimeout(() => {\r\n        let result;\r\n\r\n        if (event.query.length == 0) result = [...this.orders];\r\n        else {\r\n          result = this.orders.filter((x) => {\r\n            return x.siparisNo\r\n              .toLowerCase()\r\n              .startsWith(event.query.toLowerCase());\r\n          });\r\n        }\r\n\r\n        this.filteredOrders = result;\r\n      }, 250);\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.isMobile {\r\n  word-break: break-all;\r\n  font-size: 8px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}