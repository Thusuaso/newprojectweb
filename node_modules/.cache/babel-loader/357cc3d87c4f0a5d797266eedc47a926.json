{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport loginService from \"@/service/loginService\";\nexport default {\n  data() {\n    return {\n      user: {\n        username: null,\n        password: null\n      }\n    };\n  },\n  methods: {\n    login() {\n      this.$store.dispatch(\"loadingBeginAct\");\n      loginService.login(this.user.username, this.user.password).then(res => {\n        if (res.user.id > 0) {\n          localStorage.setItem(\"userId\", res.user.id);\n          localStorage.setItem(\"username\", res.user.kullaniciAdi);\n          this.$store.commit(\"_setUserId\", res.user.id);\n          this.$store.commit(\"_setUsername\", res.user.kullaniciAdi);\n          this.$router.push(\"/\");\n          this.$store.dispatch(\"loadingEndAct\");\n        } else {\n          alert(\"Kullanıcı bulunamadı!\");\n          this.$store.dispatch(\"loadingEndAct\");\n        }\n      });\n    }\n  },\n  created() {}\n};","map":{"version":3,"mappings":";AAeA,OAAOA,YAAW,MAAO,wBAAwB;AACjD,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,IAAI;QACdC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,KAAK,GAAG;MACN,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,iBAAiB,CAAC;MACvCR,YAAY,CAACM,KAAK,CAAC,IAAI,CAACJ,IAAI,CAACC,QAAQ,EAAE,IAAI,CAACD,IAAI,CAACE,QAAQ,CAAC,CAACK,IAAI,CAAEC,GAAG,IAAK;QACvE,IAAIA,GAAG,CAACR,IAAI,CAACS,EAAC,GAAI,CAAC,EAAE;UACnBC,YAAY,CAACC,OAAO,CAAC,QAAQ,EAAEH,GAAG,CAACR,IAAI,CAACS,EAAE,CAAC;UAC3CC,YAAY,CAACC,OAAO,CAAC,UAAU,EAAEH,GAAG,CAACR,IAAI,CAACY,YAAY,CAAC;UACvD,IAAI,CAACP,MAAM,CAACQ,MAAM,CAAC,YAAY,EAAEL,GAAG,CAACR,IAAI,CAACS,EAAE,CAAC;UAC7C,IAAI,CAACJ,MAAM,CAACQ,MAAM,CAAC,cAAc,EAAEL,GAAG,CAACR,IAAI,CAACY,YAAY,CAAC;UAEzD,IAAI,CAACE,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;UACtB,IAAI,CAACV,MAAM,CAACC,QAAQ,CAAC,eAAe,CAAC;QACvC,OAAO;UACLU,KAAK,CAAC,uBAAuB,CAAC;UAC9B,IAAI,CAACX,MAAM,CAACC,QAAQ,CAAC,eAAe,CAAC;QACvC;MACF,CAAC,CAAC;IACJ;EACF,CAAC;EACDW,OAAO,GAAG,CAAC;AACb,CAAC","names":["loginService","data","user","username","passw