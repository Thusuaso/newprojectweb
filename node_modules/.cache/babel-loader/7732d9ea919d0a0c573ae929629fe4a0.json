{"ast":null,"code":"import raporService from \"@/service/RaporService\";\nimport { mapGetters } from \"vuex\";\nimport { FilterMatchMode } from \"primevue/api\";\nexport default {\n  data() {\n    return {\n      marketing: \"Hepsi\",\n      isMobile: null,\n      uretimUrunler: null,\n      uretimUrunlerAyrinti: null,\n      filters: {\n        kategori: {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        },\n        urunAdi: {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        },\n        yuzey: {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        },\n        en: {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        },\n        boy: {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        },\n        kenar: {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        }\n      },\n      sipToplami: 0,\n      uretimToplami: 0,\n      isUretilecekForm: false,\n      sipToplamiAyrinti: 0\n    };\n  },\n  computed: {\n    ...mapGetters([\"servis_adres\"])\n  },\n  methods: {\n    changeMarketing() {\n      this.$store.dispatch(\"datatableLoadingBeginAct\");\n      if (this.marketing == \"Hepsi\") {\n        raporService.getUrunlerUretimList().then(data => {\n          this.uretimUrunler = data;\n          this.$store.dispatch(\"datatableLoadingEndAct\");\n        });\n      } else if (this.marketing == \"Mekmar\") {\n        raporService.getUrunlerUretimListMekmar().then(data => {\n          this.uretimUrunler = data;\n          this.$store.dispatch(\"datatableLoadingEndAct\");\n        });\n      } else if (this.marketing == \"Mekmer\") {\n        raporService.getUrunlerUretimListMekmer().then(data => {\n          this.uretimUrunler = data;\n          this.$store.dispatch(\"datatableLoadingEndAct\");\n        });\n      }\n    },\n    isSelectUrun(event) {\n      if (this.marketing == \"Hepsi\") {\n        raporService.getUrunlerUretimAyrintiList(event.data.urunKartId).then(data => {\n          this.uretimUrunlerAyrinti = data;\n          this.isUretilecekForm = true;\n          this.sipToplamiAyrinti = 0;\n          for (let item of data) {\n            this.sipToplamiAyrinti += item.sipMiktari;\n          }\n        });\n      } else if (this.marketing == \"Mekmar\") {\n        raporService.getUrunlerUretimAyrintiListMekmar(event.data.urunKartId).then(data => {\n          this.uretimUrunlerAyrinti = data;\n          this.isUretilecekForm = true;\n          this.sipToplamiAyrinti = 0;\n          for (let item of data) {\n            this.sipToplamiAyrinti += item.sipMiktari;\n          }\n        });\n      } else if (this.marketing == \"Mekmer\") {\n        raporService.getUrunlerUretimAyrintiListMekmer(event.data.urunKartId).then(data => {\n          this.uretimUrunlerAyrinti = data;\n          this.isUretilecekForm = true;\n          this.sipToplamiAyrinti = 0;\n          for (let item of data) {\n            this.sipToplamiAyrinti += item.sipMiktari;\n          }\n        });\n      }\n    },\n    excel_cikti_click() {\n      raporService.getUretilenSipExcell(this.uretimUrunler).then(response => {\n        if (response.status) {\n          this.musteri_loading = true;\n          const link = document.createElement(\"a\");\n          link.href = this.servis_adres + \"raporlar/listeler/uretilenSipExcelListe\";\n          link.setAttribute(\"download\", \"UretilenSip.xlsx\");\n          document.body.appendChild(link);\n          link.click();\n        }\n      });\n    },\n    formatDecimal(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    uretilecekUrunlerToplam(event) {\n      this.toplam(event.filteredValue);\n    },\n    toplam(value) {\n      this.sipToplami = 0;\n      this.uretimToplami = 0;\n      for (let i of value) {\n        this.sipToplami += i.sipMiktari;\n        this.uretimToplami += i.uretimMiktar;\n      }\n    }\n  },\n  created() {\n    this.$store.dispatch(\"loadingBeginAct\");\n    raporService.getUrunlerUretimList().then(data => {\n      this.uretimUrunler = data;\n      this.$store.dispatch(\"loadingEndAct\");\n    });\n  },\n  mounted() {}\n};","map":{"version":3,"mappings":"AAoSA,OAAOA,YAAW,MAAO,wBAAwB;AACjD,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,eAAc,QAAS,cAAc;AAC9C,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,OAAO;MAClBC,QAAQ,EAAE,IAAI;MACdC,aAAa,EAAE,IAAI;MACnBC,oBAAoB,EAAE,IAAI;MAC1BC,OAAO,EAAE;QACPC,QAAQ,EAAE;UAAEC,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY,CAAC;QACjEC,OAAO,EAAE;UAAEH,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY,CAAC;QAChEE,KAAK,EAAE;UAAEJ,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY,CAAC;QAC9DG,EAAE,EAAE;UAAEL,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY,CAAC;QAC3DI,GAAG,EAAE;UAAEN,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY,CAAC;QAC5DK,KAAK,EAAE;UAAEP,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY;MAC/D,CAAC;MACDM,UAAU,EAAE,CAAC;MACbC,aAAa,EAAE,CAAC;MAChBC,gBAAgB,EAAE,KAAK;MACvBC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGrB,UAAU,CAAC,CAAC,cAAc,CAAC;EAChC,CAAC;EAEDsB,OAAO,EAAE;IACPC,eAAe,GAAG;MAChB,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,0BAA0B;MAC/C,IAAI,IAAI,CAACtB,SAAQ,IAAK,OAAO,EAAE;QAC7BJ,YAAY,CAAC2B,oBAAoB,EAAE,CAACC,IAAI,CAAEzB,IAAI,IAAK;UACjD,IAAI,CAACG,aAAY,GAAIH,IAAI;UACzB,IAAI,CAACsB,MAAM,CAACC,QAAQ,CAAC,wBAAwB;QAE/C,CAAC,CAAC;MACJ,OAAO,IAAI,IAAI,CAACtB,SAAQ,IAAK,QAAQ,EAAE;QACrCJ,YAAY,CAAC6B,0BAA0B,EAAE,CAACD,IAAI,CAAEzB,IAAI,IAAK;UACvD,IAAI,CAACG,aAAY,GAAIH,IAAI;UACzB,IAAI,CAACsB,MAAM,CAACC,QAAQ,CAAC,wBAAwB;QAE/C,CAAC,CAAC;MACJ,OAAO,IAAI,IAAI,CAACtB,SAAQ,IAAK,QAAQ,EAAE;QACrCJ,YAAY,CAAC8B,0BAA0B,EAAE,CAACF,IAAI,CAAEzB,IAAI,IAAK;UACvD,IAAI,CAACG,aAAY,GAAIH,IAAI;UACzB,IAAI,CAACsB,MAAM,CAACC,QAAQ,CAAC,wBAAwB;QAE/C,CAAC,CAAC;MACJ;IACF,CAAC;IACDK,YAAY,CAACC,KAAK,EAAE;MAClB,IAAI,IAAI,CAAC5B,SAAQ,IAAK,OAAO,EAAE;QAC7BJ,YAAW,CACRiC