ate #body=\"slotProps\">\r\n              {{ slotProps.data.urunaditop }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"yuzeyislemtop\"\r\n            header=\"Yüzey İşlem\"\r\n            headerStyle=\"width:12px\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.yuzeyislemtop }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"entop\"\r\n            header=\"En\"\r\n            headerStyle=\"width:5px\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.entop }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"boytop\"\r\n            header=\"Boy\"\r\n            headerStyle=\"width:5px\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.boytop }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"kenartop\"\r\n            header=\"Kenar\"\r\n            headerStyle=\"width:4px\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.kenartop }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"adettop\"\r\n            header=\"Adet\"\r\n            headerStyle=\"width:5px\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.adettop }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"miktartop\"\r\n            header=\"Miktar\"\r\n            headerStyle=\"width:7px\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.miktartop }}\r\n            </template>\r\n            <template #footer>\r\n              {{ formatDecimal(miktar_toplami) }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"birimaditop\"\r\n            header=\"Birim\"\r\n            headerStyle=\"width:6px\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.birimaditop }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"durum\"\r\n            header=\"Status\"\r\n            headerStyle=\"width:6px\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.durum }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"aciklama\"\r\n            header=\"Not\"\r\n            headerStyle=\"width:6px\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.aciklama }}\r\n            </template>\r\n          </Column>\r\n          <div class=\"column is-3\">\r\n            <Button\r\n              @click=\"excel_cikti_click\"\r\n              label=\"Excel\"\r\n              class=\"p-button-primary\"\r\n            />\r\n          </div>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\nimport service from \"@/service/RaporService\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      miktar_toplami: 0,\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    ...mapGetters([\"stok_top_ayrinti_list\"]),\r\n  },\r\n  props: [\"urunid\"],\r\n\r\n  methods: {\r\n    formatDecimal(value) {\r\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\r\n      return val.toString()