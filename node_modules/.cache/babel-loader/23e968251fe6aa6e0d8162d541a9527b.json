\n            this.sipToplamiAyrinti += item.sipMiktari;\n          }\n        });\n      } else if (this.marketing == 'Mekmar') {\n        raporService.getUrunlerUretimAyrintiListMekmar(event.data.urunKartId).then(data => {\n          this.uretimUrunlerAyrinti = data;\n          this.isUretilecekForm = true;\n          this.sipToplamiAyrinti = 0;\n          for (let item of data) {\n            this.sipToplamiAyrinti += item.sipMiktari;\n          }\n        });\n      } else if (this.marketing == 'Mekmer') {\n        raporService.getUrunlerUretimAyrintiListMekmer(event.data.urunKartId).then(data => {\n          this.uretimUrunlerAyrinti = data;\n          this.isUretilecekForm = true;\n          this.sipToplamiAyrinti = 0;\n          for (let item of data) {\n            this.sipToplamiAyrinti += item.sipMiktari;\n          }\n        });\n      }\n    },\n    excel_cikti_click() {\n      raporService.getUretilenSipExcell(this.uretimUrunler).then(response => {\n        if (response.status) {\n          this.musteri_loading = true;\n          const link = document.createElement(\"a\");\n          link.href = this.servis_adres + \"raporlar/listeler/uretilenSipExcelListe\";\n          link.setAttribute(\"download\", \"UretilenSip.xlsx\");\n          document.body.appendChild(link);\n          link.click();\n        }\n      });\n    },\n    formatDecimal(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    uretilecekUrunlerToplam(event) {\n      this.toplam(event.filteredValue);\n    },\n    toplam(value) {\n      this.sipToplami = 0;\n      this.uretimToplami = 0;\n      for (let i of value) {\n        this.sipToplami += i.sipMiktari;\n        this.uretimToplami += i.uretimMiktar;\n      }\n    }\n  },\n  created() {\n    this.$store.dispatch(\"loadingBeginAct\");\n    raporService.getUrunlerUretimList().then(data => {\n      this.uretimUrunler = data;\n      this.$store.dispatch(\"loadingEndAct\");\n    });\n  },\n  mounted() {}\n};","map":{"version":3,"mappings":"AAsPA,OAAOA,YAAW,MAAO,wBAAwB;AACjD,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,eAAc,QAAS,cAAc;AAC9C,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,IAAI;MACfC,QAAQ,EAAE,IAAI;MACdC,aAAa,EAAE,IAAI;MACnBC,oBAAoB,EAAE,IAAI;MAC1BC,OAAO,EAAE;QACPC,QAAQ,EAAE;UAAEC,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY,CAAC;QACjEC,OAAO,EAAE;UAAEH,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY,CAAC;QAChEE,KAAK,EAAE;UAAEJ,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY,CAAC;QAC9DG,EAAE,EAAE;UAAEL,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY,CAAC;QAC3DI,GAAG,EAAE;UAAEN,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY,CAAC;QAC5DK,KAAK,EAAE;UAAEP,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAET,eAAe,CAACU;QAAY;MAG/D,CAAC;MACDM,UAAU,EAAE,CAAC;MACbC,aAAa,EAAE,CAAC;MAChBC,gBAAgB,EAAE,KAAK;MACvBC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGrB,UAAU,CAAC,CAAC,cAAc,CAAC;EAChC,CAAC;EAEDsB,OAAO,EAAE;IACPC,eAAe,GAAG;MAChB,IAAI,IAAI,CAACpB,SAAQ,IAAK,OAAO,EAAE;QAC7BJ,YAAY,CAACyB,oBAAoB,EAAE,CAACC,IAAI,CAACvB,IAAG,IAAK;UAC/C,IAAI,CAACG,aAAY,GAAIH,IAAG;QAC1B,CAAC;MACH,OAAO,IAAI,IAAI,CAACC,SAAQ,IAAK,QAAQ,EAAE;QACrCJ,YAAY,CAAC2B,0BAA0B,EAAE,CAACD,IAAI,CAACvB,IAAG,IAAK;UACrD,IAAI,CAACG,aAAY,GAAIH,IAAG;QAC1B,CAAC;MACH,OAAO,IAAI,IAAI,CAACC,SAAQ,IAAK,QAAQ,EAAE;QACrCJ,YAAY,CAAC4B,0BAA0B,EAAE,CAACF,IAAI,CAACvB,IAAG,IAAK;UACrD,IAAI,CAACG,aAAY,GAAIH,IAAG;QAC1B,CAAC;MACH;IACF,CAAC;IACD0B,YAAY,CAACC,KAAK,EAAE;MAClB,IAAI,IAAI,CAAC1B,SAAQ,IAAK,OAAO,EAAE;QAC7BJ,YAAW,CACR+B,2BAA2B,CAACD,KAAK,CAAC3B,IAAI,CAAC6B,UAAU,EACjDN,IAAI,CAAEvB,IAAI,IAAK;UAChB,IAAI,CAACI,oBAAmB,GAAIJ,IAAG;UAC/B,IAAI,CAACiB,gBAAe,GAAI,IAAG;UAC3B,IAAI,CAACC,iBAAgB,GAAI;UACzB,KAAK,IAAIY,IAAG,IAAK9B,IAAI,EAAE;YACrB,IAAI,CAACkB,iBAAgB,IAAKY,IAAI,CAACC,UAAS;UAC1C;QACF,CAAC;MACH,OACK,IAAI,IAAI,CAAC9B,SAAQ,IAAK,QAAQ,EAAE;QACnCJ,YAAY,CAACmC,iCAAiC,CAACL,KAAK,CAAC3B,IAAI,CAAC6B,UAAU,CAAC,CAACN,IAAI,CAACvB,IAAG,IAAK;UACjF,IAAI,CAACI,oBAAmB,GAAIJ,IAAG;UAC/B,IAAI,