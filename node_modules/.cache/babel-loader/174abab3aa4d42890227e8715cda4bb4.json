 {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    uretilecekUrunlerToplam(event) {\n      this.toplam(event.filteredValue);\n    },\n    toplam(value) {\n      this.sipToplami = 0;\n      this.uretimToplami = 0;\n      for (let i of value) {\n        this.sipToplami += i.sipMiktari;\n        this.uretimToplami += i.uretimMiktar;\n      }\n    }\n  },\n  created() {\n    this.$store.dispatch(\"loadingBeginAct\");\n    raporService.getUrunlerUretimList().then(data => {\n      this.uretimUrunler = data;\n      this.$store.dispatch(\"loadingEndAct\");\n    });\n  },\n  mounted() {}\n};","map":{"version":3,"mappings":"AAkPA,OAAOA,YAAW,MAAO,wBAAwB;AACjD,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,eAAc,QAAS,cAAc;AAC9C,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,IAAI;MACdC,aAAa,EAAE,IAAI;MACnBC,oBAAoB,EAAE,IAAI;MAC1BC,OAAO,EAAE;QACPC,QAAQ,EAAE;UAAEC,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAER,eAAe,CAACS;QAAY,CAAC;QACjEC,OAAO,EAAE;UAAEH,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAER,eAAe,CAACS;QAAY,CAAC;QAChEE,KAAK,EAAE;UAAEJ,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAER,eAAe,CAACS;QAAY,CAAC;QAC9DG,EAAE,EAAE;UAAEL,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAER,eAAe,CAACS;QAAY,CAAC;QAC3DI,GAAG,EAAE;UAAEN,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAER,eAAe,CAACS;QAAY,CAAC;QAC5DK,KAAK,EAAE;UAAEP,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAER,eAAe,CAACS;QAAY;MAG/D,CAAC;MACDM,UAAU,EAAE,CAAC;MACbC,aAAa,EAAE,CAAC;MAChBC,gBAAgB,EAAE,KAAK;MACvBC,iBAAiB,EAAE;IACrB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGpB,UAAU,CAAC,CAAC,cAAc,CAAC;EAChC,CAAC;EAEDqB,OAAO,EAAE;IACPC,YAAY,CAACC,KAAK,EAAE;MAClBxB,YAAW,CACRyB,2BAA2B,CAACD,KAAK,CAACrB,IAAI,CAACuB,UAAU,EACjDC,IAAI,CAAExB,IAAI,IAAK;QACd,IAAI,CAACG,oBAAmB,GAAIH,IAAI;QAChC,IAAI,CAACgB,gBAAe,GAAI,IAAI;QAC5B,IAAI,CAACC,iBAAgB,GAAI,CAAC;QAC1B,KAAK,IAAIQ,IAAG,IAAKzB,IAAI,EAAE;UACrB,IAAI,CAACiB,iBAAgB,IAAKQ,IAAI,CAACC,UAAU;QAC3C;MACF,CAAC,CAAC;IACN,CAAC;IACDC,iBAAiB,GAAG;MAClB9B,YAAY,CAAC+B,oBAAoB,CAAC,IAAI,CAAC1B,aAAa,CAAC,CAACsB,IAAI,CAAEK