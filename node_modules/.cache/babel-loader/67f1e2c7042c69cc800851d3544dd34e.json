              />\r\n            </template>\r\n          </Column>\r\n          <Column field=\"kimden\" header=\"Kimden\" headerStyle=\"width:14%\">\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.kimden }}\r\n            </template>\r\n            <template #filter>\r\n              <InputText\r\n                type=\"text\"\r\n                v-model=\"filters['kimden']\"\r\n                class=\"p-column-filter\"\r\n              />\r\n            </template>\r\n          </Column>\r\n\r\n          <Column\r\n            field=\"urunKartID\"\r\n            header=\"Ürün Kart Id\"\r\n            headerStyle=\"width:14%\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.urunKartID }}\r\n            </template>\r\n            <template #filter>\r\n              <InputText\r\n                type=\"text\"\r\n                v-model=\"filters['urunKartID']\"\r\n                class=\"p-column-filter\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column field=\"kategori\" header=\"Kategori\" headerStyle=\"width:14%\">\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.kategori }}\r\n            </template>\r\n            <template #filter>\r\n              <InputText\r\n                type=\"text\"\r\n                v-model=\"filters['kategori']\"\r\n                class=\"p-column-filter\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"kasano\"\r\n            header=\"No\"\r\n            headerStyle=\"width:13%\"\r\n            bodyStyle=\"text-align:center\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.kasano }}\r\n            </template>\r\n            <template #filter>\r\n              <InputText\r\n                type=\"text\"\r\n                v-model=\"filters['kasano']\"\r\n              