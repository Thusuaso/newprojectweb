{"ast":null,"code":"import service from \"../../service/RaporService\";\nimport { mapGetters } from \"vuex\";\nimport { FilterMatchMode } from \"primevue/api\";\nexport default {\n  data() {\n    return {\n      digerMasraflar: null,\n      selectedYear: 2022,\n      years: [{\n        year: 2022\n      }, {\n        year: 2021\n      }, {\n        year: 2020\n      }],\n      topSip: null,\n      topNavlun: null,\n      topDiger1: null,\n      topDiger2: null,\n      topDiger3: null,\n      sizeBody: null,\n      filters: {\n        musteriAdi: {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        }\n      }\n    };\n  },\n  computed: {\n    ...mapGetters([\"servis_adres\"])\n  },\n  methods: {\n    isDigerMasraflar(event) {\n      this.miktarTopla(event.filteredValue);\n    },\n    digerMasraflarService(year) {\n      service.getDigerMasraflar(year).then(data => {\n        this.digerMasraflar = data;\n        this.miktarTopla(data);\n      });\n    },\n    isSelectedYear(event) {\n      this.digerMasraflarService(event.value.year);\n    },\n    formatDecimal(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    formatPrice(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return \"$\" + val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    miktarTopla(data) {\n      this.topSip = 0;\n      this.topNavlun = 0;\n      this.topDiger1 = 0;\n      this.topDiger2 = 0;\n      this.topDiger3 = 0;\n      for (let item of data) {\n        this.topSip += item.siparisTotal;\n        this.topNavlun += item.navlunSatis;\n        this.topDiger1 += item.detay_1;\n        this.topDiger2 += item.detay_2;\n        this.topDiger3 += item.detay_3;\n      }\n    },\n    get_digermasraflar_excell_cikti_btn() {\n      service.getDigerMasraflarExcel(this.digerMasraflar).then(response => {\n        if (response.status) {\n          const link = document.createElement(\"a\");\n          link.href = this.servis_adres + \"islemler/listeler/digerMasraflarExcel\";\n          link.setAttribute(\"download\", \"digerMasraflarExcel.xlsx\");\n          document.body.appendChild(link);\n          link.click();\n          this.is_excel = false;\n        }\n      });\n    }\n  },\n  created() {\n    this.digerMasraflarService(this.selectedYear);\n    this.selectedYear = {\n      year: 2022\n    };\n    const width = window.innerWidth;\n    if (width >= 500) {\n      this.sizeBody = 9;\n    }\n  }\n};","map":{"version":3,"mappings":"AA2HA,OAAOA,OAAM,MAAO,4BAA4B;AAChD,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,eAAc,QAAS,cAAc;AAC9C,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,cAAc,EAAE,IAAI;MACpBC,YAAY,EAAE,IAAI;MAClBC,KAAK,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAK,CAAC,EAAE;QAAEA,IAAI,EAAE;MAAK,CAAC,EAAE;QAAEA,IAAI,EAAE;MAAK,CAAC,CAAC;MACvDC,MAAM,EAAE,IAAI;MACZC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE,IAAI;MACfC,SAAS,EAAE,IAAI;MACfC,QAAQ,EAAE,IAAI;MACdC,OAAO,EAAE;QACPC,UAAU,EAAE;UAAEC,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAEf,eAAe,CAACgB;QAAY;MACpE;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGlB,UAAU,CAAC,CAAC,cAAc,CAAC;EAChC,CAAC;EACDmB,OAAO,EAAE;IACPC,gBAAgB,CAACC,KAAK,EAAE;MACtB,IAAI,CAACC,WAAW,CAACD,KAAK,CAACE,aAAa;IACtC,CAAC;IACDC,qBAAqB,CAAClB,IAAI,EAAE;MAC1BP,OAAO,CAAC0B,iBAAiB,CAACnB,IAAI,CAAC,CAACoB,IAAI,CAAExB,IAAI,IAAK;QAC7C,IAAI,CAACC,cAAa,GAAID,IAAI;QAC1B,IAAI,CAACoB,WAAW,CAACpB,IAAI,CAAC;MACxB,CAAC,CAAC;IACJ,CAAC;IACDyB,cAAc,CAACN,KAAK,EAAE;MACpB,IAAI,CAACG,qBAAqB,CAACH,KAAK,CAACN,KAAK,CAACT,IAAI,CAAC;IAC9C,CAAC;IACDsB,aAAa,CAACb,KAAK,EAAE;MACnB,IAAIc,GAAE,GAAI,CAACd,KAAI,GAAI,CAAC,EAAEe,OAAO,CAAC,CAAC,CAAC,CAACC,OAAO,CAAC,GAAG,EAAE,GAAG,CAAC;MAClD,OAAOF,GAAG,CAACG,QAAQ,EAAE,CAACD,OAAO,CAAC,uBAAuB,EAAE,GAAG,CAAC;IAC7D,CAAC;IAEDE,WAAW,CAAClB,KAAK,EAAE;MACjB,IAAIc,GAAE,GAAI,CAACd,KAAI,GAAI,CAAC,EAAEe,OAAO,CAAC,CAAC,CAAC,CAACC,OAAO,CAAC,GAAG,EAAE,GAAG,CAAC;MAClD,OAAO,GAAE,GAAIF,GAAG,CAACG,QAAQ,EAAE,CAA