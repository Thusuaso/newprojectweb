{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport depoKullanimService from \"@/service/DepoKullanimService\";\nexport default {\n  data() {\n    return {\n      kullanimList: null,\n      kullanim: null,\n      silinenler: [],\n      eklenenler: []\n    };\n  },\n  props: [\"urunid\"],\n  created() {\n    depoKullanimService.getKullanimList(this.urunid).then(data => {\n      this.kullanimList = data;\n    });\n  },\n  methods: {\n    durumDegisim(item, event) {\n      if (event) {\n        this.urunEkle(item);\n      } else {\n        this.urunSil(item);\n      }\n    },\n    urunEkle(item) {\n      const data = {\n        ...item,\n        urunid: this.urunid\n      };\n      this.eklenenler.push(data);\n      this.kontrol(this.silinenler, item);\n    },\n    urunSil(item) {\n      const data = {\n        ...item,\n        urunid: this.urunid\n      };\n      this.silinenler.push(data);\n      this.kontrol(this.eklenenler, item);\n    },\n    kontrol(list, item) {\n      for (let key in list) {\n        if (list[key].id == item.id) {\n          list.splice(key, 1);\n        }\n      }\n    },\n    dataKayitIslem() {\n      const data = {\n        eklenenler: this.eklenenler,\n        silinenler: this.silinenler\n      };\n      depoKullanimService.dataKayitIslem(data).then(status => {\n        if (status) {\n          this.eklenenler = [];\n          this.silinenler = [];\n          alert(\"Kullan覺m Alanlar覺 Eklendi.\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AAmBA,OAAOA,mBAAkB,MAAO,+BAA+B;AAE/D,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,IAAI;MAClBC,QAAQ,EAAE,IAAI;MACdC,UAAU,EAAE,EAAE;MACdC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,KAAK,EAAE,CAAC,QAAQ,CAAC;EACjBC,OAAO,GAAG;IACRP,mBAAmB,CAACQ,eAAe,CAAC,IAAI,CAACC,MAAM,CAAC,CAACC,IAAI,CAAET,IAAI,IAAK;MAC9D,IAAI,CAACC,YAAW,GAAID,IAAI;IAC1B,CAAC,CAAC;EACJ,CAAC;EACDU,OAAO,EAAE;IACPC,YAAY,CAACC,IAAI,EAAEC,KAAK,EAAE;MACxB,IAAIA,KAAK,EAAE;QACT,IAAI,CAACC,QAAQ,CAACF,IAAI,