\"\r\n            header=\"Telefon\"\r\n            headerStyle=\"width:5px\"\r\n            bodyStyle=\"text-align:left\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <div style=\"margin-left: -9px; word-break: break-all\">\r\n                {{ slotProps.data.telefon }}\r\n              </div>\r\n            </template>\r\n            <template #filter>\r\n              <InputText\r\n                type=\"text\"\r\n                v-model=\"filters['telefon']\"\r\n                class=\"p-column-filter\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"mail\"\r\n            header=\"Mail\"\r\n            headerStyle=\"width:8px\"\r\n            bodyStyle=\"text-align:left\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.mail }}\r\n            </template>\r\n            <template #filter>\r\n              <InputText\r\n                type=\"text\"\r\n                v-model=\"filters['mail']\"\r\n                class=\"p-column-filter\"\r\n              />\r\n            </template>\r\n          </Column>\r\n\r\n          <Column\r\n            field=\"aciklama\"\r\n            header=\"Not\"\r\n            headerStyle=\"width:5px\"\r\n            bodyStyle=\"text-align:left\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              {{ slotProps.data.aciklama }}\r\n            </template>\r\n            <template #filter>\r\n              <InputText\r\n                type=\"text\"\r\n                v-model=\"filters['aciklama']\"\r\n                class=\"p-column-filter\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n<script>\r\nimport service from \"@/service/OperasyonService\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      isMobile: null,\r\n      filters: {},\r\n    