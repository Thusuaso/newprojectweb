_maliyet_form = true;\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.p-datatable {\r\n  font-size: 10px;\r\n}\r\n\r\n.p-column-filter {\r\n  font-size: 10px;\r\n}\r\n\r\n.page-button {\r\n  margin-top: 25px;\r\n}\r\n\r\n.page-button-excell {\r\n  margin-left: -100px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}