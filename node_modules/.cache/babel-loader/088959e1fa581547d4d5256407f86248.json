          field=\"ocakAdi\"\r\n              header=\"Ocak Adı\"\r\n              headerStyle=\"width:80px\"\r\n              bodyStyle=\"text-align:center\"\r\n            >\r\n              <template style=\"display: sticky\" #body=\"slotProps\">\r\n                {{ slotProps.data.ocakAdi }}\r\n              </template>\r\n            </Column>\r\n\r\n            <Column\r\n              field=\"mt2\"\r\n              header=\"Mt2\"\r\n              headerStyle=\"width:70px\"\r\n              bodyStyle=\"text-align:center\"\r\n            >\r\n              <template style=\"display: sticky\" #body=\"slotProps\">\r\n                {{ slotProps.data.mt2 }}\r\n              </template>\r\n            </Column>\r\n\r\n            <Column\r\n              field=\"mt\"\r\n              header=\"Mt\"\r\n              headerStyle=\"width:70px\"\r\n              bodyStyle=\"text-align:center\"\r\n            >\r\n              <template style=\"display: sticky\" #body=\"slotProps\">\r\n                {{ slotProps.data.mt }}\r\n              </template>\r\n            </Column>\r\n\r\n            <Column\r\n              field=\"adet\"\r\n              header=\"Adet\"\r\n              headerStyle=\"width:70px\"\r\n              bodyStyle=\"text-align:center\"\r\n            >\r\n              <template style=\"display: sticky\" #body=\"slotProps\">\r\n                {{ slotProps.data.adet }}\r\n              </template>\r\n            </Column>\r\n\r\n            <Column\r\n              field=\"kasaSayisi\"\r\n              header=\"Kasa Sayısı\"\r\n              headerStyle=\"width:70px\"\r\n              bodyStyle=\"text-align:center\"\r\n            >\r\n              <template style=\"display: sticky\" #body=\"slotProps\">\r\n                {{ slotProps.data.kasaSayisi }}\r\n              </template>\r\n            </Column>\r\n          </DataTable>\r\n        </div>\r\n\r\n        <Dialog\r\n          v-model:visible=\"is_maliyet_form\"\r\n          header=\"O