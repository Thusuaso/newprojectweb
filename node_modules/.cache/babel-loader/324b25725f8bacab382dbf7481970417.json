tch(\"depo_ayrinti_list_yukle_act\", data);\r\n        this.form_baslik = event.data.musteriadi + \" Borç Alacak Listesi\";\r\n        this.is_form = true;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.p-datatable {\r\n  font-size: 10px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}