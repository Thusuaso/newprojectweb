{"ast":null,"code":"import service from \"@/service/RaporService\";\nimport { mapGetters } from \"vuex\";\nimport { FilterMatchMode } from \"primevue/api\";\nexport default {\n  data() {\n    return {\n      nakliyeData: null,\n      selectedYear: 2022,\n      years: [{\n        year: 2022\n      }, {\n        year: 2021\n      }, {\n        year: 2020\n      }],\n      topNakliyeData: null,\n      toplamTl: null,\n      toplamDolar: null,\n      filters: {\n        firma: {\n          value: null,\n          matchMode: FilterMatchMode.STARTS_WITH\n        }\n      }\n    };\n  },\n  computed: {\n    ...mapGetters([\"servis_adres\"])\n  },\n  methods: {\n    isNakliyeMasraflari(event) {\n      console.log(event.filteredValue);\n      this.toplamTl = 0;\n      this.toplamDolar = 0;\n      for (let item in event.filteredValue) {\n        this.toplamTl += event.filteredValue[item].tl;\n        this.toplamDolar += event.filteredValue[item].dolar;\n      }\n    },\n    nakliyeciData(year) {\n      service.getNakliyeciDagilimi(year).then(data => {\n        this.nakliyeData = data;\n        this.sum(data);\n      });\n    },\n    sum(data) {\n      this.toplamTl = 0;\n      this.toplamDolar = 0;\n      for (let item of data) {\n        this.toplamTl += item.tl;\n        this.toplamDolar += item.dolar;\n      }\n    },\n    isSelectedYear(event) {\n      this.nakliyeciData(event.value.year);\n    },\n    formatDecimal(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    formatPrice(value) {\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\n      return \"$\" + val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\n    },\n    get_nakliye_excell_cikti_btn() {\n      service.getNakliyeciDagilimiExcel(this.nakliyeData).then(response => {\n        if (response.status) {\n          const link = document.createElement(\"a\");\n          link.href = this.servis_adres + \"islemler/listeler/nakliyeciDagilimiExcel\";\n          link.setAttribute(\"download\", \"nakliyeciDagilimiExcel.xlsx\");\n          document.body.appendChild(link);\n          link.click();\n          this.is_excel = false;\n        }\n      });\n    }\n  },\n  created() {\n    this.nakliyeciData(this.selectedYear);\n    this.selectedYear = {\n      year: 2022\n    };\n  }\n};","map":{"version":3,"mappings":"AA4EA,OAAOA,OAAM,MAAO,wBAAwB;AAC5C,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,eAAc,QAAS,cAAc;AAC9C,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,IAAI;MACjBC,YAAY,EAAE,IAAI;MAClBC,KAAK,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAK,CAAC,EAAE;QAAEA,IAAI,EAAE;MAAK,CAAC,EAAE;QAAEA,IAAI,EAAE;MAAK,CAAC,CAAC;MACvDC,cAAc,EAAE,IAAI;MACpBC,QAAQ,EAAE,IAAI;MACdC,WAAW,EAAE,IAAI;MACjBC,OAAO,EAAE;QACPC,KAAK,EAAE;UAAEC,KAAK,EAAE,IAAI;UAAEC,SAAS,EAAEZ,eAAe,CAACa;QAAY;MAC/D;IACF,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGf,UAAU,CAAC,CAAC,cAAc,CAAC;EAChC,CAAC;EACDgB,OAAO,EAAE;IACPC,mBAAmB,CAACC,KAAK,EAAE;MACzBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAACG,aAAa;MAC/B,IAAI,CAACb,QAAO,GAAI;MAChB,IAAI,CAACC,WAAU,GAAI;MACnB,KAAK,IAAIa,IAAG,IAAKJ,KAAK,CAACG,aAAa,EAAE;QACpC,IAAI,CAACb,QAAO,IAAKU,KAAK,CAACG,aAAa,CAACC,IAAI,CAAC,CAACC,EAAC;QAC5C,IAAI,CAACd,WAAU,IAAKS,KAAK,CAACG,aAAa,CAACC,IAAI,CAAC,CAACE,KAAI;MAEpD;IACF,CAAC;IAEDC,aAAa,CAACnB,IAAI,EAAE;MAClBP,OAAO,CAAC2B,oBAAoB,CAACpB,IAAI,CAAC,CAACqB,IAAI,CAAEzB,IAAI,IAAK;QAChD,IAAI,CAACC,WAAU,GAAID,IAAI;QACvB,IAAI,CAAC0B,GAAG,CAAC1B,IAAI,CAAC;MAChB,CAAC,CAAC;IACJ,CAAC;IACD0B,GAAG,CAAC1B,IAAI,EAAE;MACR,IAAI,CAACM,QAAO,GAAI,CAAC;MACjB,IAAI,CAACC,WAAU,GAAI,CAAC;MACpB,KAAK,IAAIa,IAAG,IAAKpB,IAAI,EAAE;QACrB,IAAI,CAACM,QAAO,IAAKc,IAAI,CAACC,EAAE;QACxB,IAAI,CAACd,WAAU,IAAKa,IAAI,CAACE,KAAK;MAChC;IACF,CAAC;IACDK,cAAc,CAACX,KAAK,EAAE;MACpB,IAAI,CAACO,aAAa,CAACP,KAAK,CAACN,KAAK,CAACN,IAAI,CAAC;IACtC,CAAC;IACDwB,aAAa,CAAClB,KAAK,EAAE;MACnB,IAAImB,GAAE,GAAI,CAACnB,KAAI,GAAI,CAAC,EAAEoB,OAAO,CAAC,CAAC,CAAC,CAACC,OAAO,CAAC,GAAG,EAAE,GAAG,CAAC;MAClD,OAAOF,GAAG,CAACG,QAAQ,EAAE,CAACD,OAAO,CAAC,uBAAuB,EAAE,GAAG,CAAC;IAC7D,CAAC;IAEDE,WAAW,CAACvB,KAAK,EAAE;MACjB,IAAImB,GAAE,GAAI,CAACnB,KAAI,GAAI,CAAC,EAAEoB,OAAO,CAAC,CAAC,CAAC,CAACC,OAAO,CAAC,GAAG,EAAE,GAAG,CAAC;MAClD,OAAO,GAAE,GAAIF,GAAG,CAACG,QAAQ,EAAE,CAACD,OAAO,CAAC,uBAAuB,EAAE,GAAG,CAAC;IACnE,CAAC;IACDG,4BAA4B,GAAG;MAC7BrC,OAAO,CAACsC,yBAAyB,CAAC,IAAI,CAAClC,WAAW,CAAC,CAACwB,IAAI,CAAEW,QAAQ,IAAK;QACrE,IAAIA,QAAQ,CAACC,MAAM,EAAE;UACnB,MAAMC,IAAG,GAAIC,QAAQ,CAACC,aAAa,CAAC,GAAG,CAAC;UACxCF,IAAI,CAACG,IAAG,GACN,IAAI,CAACC,YAAW,GAAI,0CAA0C;UAChEJ,IAAI,CAACK,YAAY,CAAC,UAAU,EAAE,6BAA6B,CAAC;UAC5DJ,QAAQ,CAACK,IAAI,CAACC,WAAW,CAACP,IAAI,CAAC;UAC/BA,IAAI,CAACQ,KAAK,EAAE;UACZ,IAAI,CAACC,QAAO,GAAI,KAAK;QACvB;MACF,CAAC,CAAC;IACJ;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACzB,aAAa,CAAC,IAAI,CAACrB,YAAY,CAAC;IACrC,IAAI,CAACA,YAAW,GAAI;MAAEE,IAAI,EAAE;IAAK,CAAC;EACpC;AACF,CAAC","names":["service","mapGetters","FilterMatchMode","data","nakliyeData","selectedYear","years","year","topNakliyeData","toplamTl","toplamDolar","filters","firma","value","matchMode","STARTS_WITH","computed","methods","isNakliyeMasraflari","event","console","log","filteredValue","item","tl","dolar","nakliyeciData","getNakliyeciDagilimi","then","sum","isSelectedYear","formatDecimal","val","toFixed","replace","toString","formatPrice","get_nakliye_excell_cikti_btn","getNakliyeciDagilimiExcel","response","status","link","document","createElement","href","servis_adres","setAttribute","body","appendChild","click","is_excel","created"],"sourceRoot":"","sources":["C:\\PROJELER\\NewProjects\\gitPull\\newprojectweb\\src\\components\\musteriraporlari\\NakliyeMasrafları.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"columns\" style=\"margin-top: 10px; margin-left: 15px\">\r\n      <Dropdown\r\n        v-model=\"selectedYear\"\r\n        :options=\"years\"\r\n        optionLabel=\"year\"\r\n        placeholder=\"Select a Year\"\r\n        @change=\"isSelectedYear\"\r\n      />\r\n\r\n      <b-button\r\n        style=\"width: 10px; height: 32px; margin-left: 10px\"\r\n        @click=\"get_nakliye_excell_cikti_btn\"\r\n        ><i class=\"fa-solid fa-file-excel fa-2xl\"></i>\r\n      </b-button>\r\n    </div>\r\n\r\n    <DataTable\r\n      :value=\"nakliyeData\"\r\n      :scrollable=\"true\"\r\n      scrollHeight=\"400px\"\r\n      v-model:filters=\"filters\"\r\n      filterDisplay=\"menu\"\r\n      @filter=\"isNakliyeMasraflari\"\r\n    >\r\n      <Column\r\n        field=\"firma\"\r\n        header=\"Firma Adı\"\r\n        headerStyle=\"width:30%;\"\r\n        bodyStyle=\"text-align:center;\"\r\n      >\r\n        <template #body=\"slotProps\">\r\n          {{ slotProps.data.firma }}\r\n        </template>\r\n        <template #filter=\"{ filterModel, filterCallback }\">\r\n          <InputText\r\n            type=\"text\"\r\n            v-model=\"filterModel.value\"\r\n            @input=\"filterCallback()\"\r\n            class=\"p-column-filter\"\r\n            placeholder=\"Search by Customer\"\r\n          />\r\n        </template>\r\n      </Column>\r\n      <Column\r\n        field=\"tl\"\r\n        header=\"Nakliye Toplamı (₺)\"\r\n        headerStyle=\"width:30%;\"\r\n        bodyStyle=\"text-align:center;\"\r\n      >\r\n        <template #body=\"slotProps\">\r\n          {{ formatPrice(slotProps.data.tl) }}\r\n        </template>\r\n        <template #footer>\r\n          {{ formatPrice(toplamTl) }}\r\n        </template>\r\n      </Column>\r\n      <Column\r\n        field=\"dolar\"\r\n        header=\"Nakliye Toplamı ($)\"\r\n        headerStyle=\"width:30%;\"\r\n        bodyStyle=\"text-align:center;\"\r\n      >\r\n        <template #body=\"slotProps\">\r\n          {{ formatPrice(slotProps.data.dolar) }}\r\n        </template>\r\n        <template #footer>\r\n          {{ formatPrice(toplamDolar) }}\r\n        </template>\r\n      </Column>\r\n    </DataTable>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport service from \"@/service/RaporService\";\r\nimport { mapGetters } from \"vuex\";\r\nimport { FilterMatchMode } from \"primevue/api\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      nakliyeData: null,\r\n      selectedYear: 2022,\r\n      years: [{ year: 2022 }, { year: 2021 }, { year: 2020 }],\r\n      topNakliyeData: null,\r\n      toplamTl: null,\r\n      toplamDolar: null,\r\n      filters: {\r\n        firma: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"servis_adres\"]),\r\n  },\r\n  methods: {\r\n    isNakliyeMasraflari(event) {\r\n      console.log(event.filteredValue)\r\n      this.toplamTl = 0\r\n      this.toplamDolar = 0\r\n      for (let item in event.filteredValue) {\r\n        this.toplamTl += event.filteredValue[item].tl\r\n        this.toplamDolar += event.filteredValue[item].dolar\r\n\r\n      }\r\n    },\r\n\r\n    nakliyeciData(year) {\r\n      service.getNakliyeciDagilimi(year).then((data) => {\r\n        this.nakliyeData = data;\r\n        this.sum(data);\r\n      });\r\n    },\r\n    sum(data) {\r\n      this.toplamTl = 0;\r\n      this.toplamDolar = 0;\r\n      for (let item of data) {\r\n        this.toplamTl += item.tl;\r\n        this.toplamDolar += item.dolar;\r\n      }\r\n    },\r\n    isSelectedYear(event) {\r\n      this.nakliyeciData(event.value.year);\r\n    },\r\n    formatDecimal(value) {\r\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\r\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\r\n    },\r\n\r\n    formatPrice(value) {\r\n      let val = (value / 1).toFixed(2).replace(\".\", \",\");\r\n      return \"$\" + val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\");\r\n    },\r\n    get_nakliye_excell_cikti_btn() {\r\n      service.getNakliyeciDagilimiExcel(this.nakliyeData).then((response) => {\r\n        if (response.status) {\r\n          const link = document.createElement(\"a\");\r\n          link.href =\r\n            this.servis_adres + \"islemler/listeler/nakliyeciDagilimiExcel\";\r\n          link.setAttribute(\"download\", \"nakliyeciDagilimiExcel.xlsx\");\r\n          document.body.appendChild(link);\r\n          link.click();\r\n          this.is_excel = false;\r\n        }\r\n      });\r\n    },\r\n  },\r\n  created() {\r\n    this.nakliyeciData(this.selectedYear);\r\n    this.selectedYear = { year: 2022 };\r\n  },\r\n};\r\n</script>\r\n<style scoped></style>\r\n"]},"metadata":{},"sourceType":"module"}